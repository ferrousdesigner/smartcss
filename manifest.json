{
    "name": "Smartcss",
    "description" : "An extension that let's you tweak the styles of an element easily.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {"128": "smartcss.png"},
    "browser_action": {
        "default_popup": "smartcss.html",
        "default_icon": "smartcss_19px.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ],
    "permissions": [
        "tabs"
    ]
}